{% extends 'base.html.twig' %}

{% block title %}Map{% endblock %}

{% block body %}
    <h1>Map</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ map.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ map.name }}</td>
            </tr>
            <tr>
                <th>DisplayName</th>
                <td>{{ map.displayName }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ map.url }}</td>
            </tr>
            <tr>
                <th>MinZoom</th>
                <td>{{ map.minZoom }}</td>
            </tr>
            <tr>
                <th>MaxZoom</th>
                <td>{{ map.maxZoom }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('cpd_map_index') }}">back to list</a>

    <a href="{{ path('cpd_map_edit', {'id': map.id}) }}">edit</a>

    {{ include('map/_delete_form.html.twig') }}
{% endblock %}
